<h1>Post List</h1>

<table class="table table-bordered">
  <thead class="bg-info">
    <tr>
      <td>ID</td>
      <td>Title</td>
      <td>Content</td>
      <td>User</td>
      <td>Action</td>
    </tr>
  </thead>
  <% @posts.each do |post| %>
    <tr>
      <td><%= post.id %></td>
      <td><%= post.title %></td>
      <td><%= post.content %></td>
      <td><%= post.login.email %></td>
      <td><%=  %></td>
    </tr>
    <% end %>
</table>

<%= link_to 'New Post', new_post_path%>